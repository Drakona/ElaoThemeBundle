<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="elao.twig.theme_extension.class">Elao\Bundle\ThemeBundle\Twig\ElaoThemeExtension</parameter>
    </parameters>

    <services>
        <service id="elao.twig.theme_extension" class="%elao.twig.theme_extension%">
            <tag name="twig.extension" />
        </service>
    </services>
</container>
